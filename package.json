{
  "name": "charly",
  "version": "0.3.0",
  "description": "A golfing language for string processing",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "scripts": {
    "clean-dist": "rimraf dist",
    "build": "npm run clean-dist && tsc",
    "lint": "tslint --project .",
    "mocha": "mocha",
    "test": "npm run lint && npm run mocha",
    "genDocs": "npm run generateCommandDoc && npm run generateVariableDoc",
    "addCommand": "ts-node tools/addCommand.ts",
    "tsShell": "ts-node -r tsconfig-paths/register",
    "generateCommandDoc": "ts-node -r tsconfig-paths/register tools/generateCommandDoc.ts",
    "generateVariableDoc": "ts-node -r tsconfig-paths/register tools/generateVariableDoc.ts"
  },
  "author": "Jannis Isensee",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.17",
    "@types/prompts": "^1.2.0",
    "chai": "^3.5.0",
    "install": "^0.12.2",
    "mocha": "^5.2.0",
    "npm": "^6.5.0",
    "prompts": "^2.0.0",
    "rimraf": "^2.5.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.2.1"
  },
  "dependencies": {
    "@types/cli": "^0.11.19",
    "cli": "^1.0.1"
  }
}
